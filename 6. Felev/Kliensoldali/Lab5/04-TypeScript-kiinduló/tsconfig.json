{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "sourceMap": true,
    "lib": [
      "es2015",
      "dom"
    ],
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "moduleResolution": "node",
    "inlineSourceMap": true
  },
  "typeAcquisition": {
    "enable": true
  },
  "exclude": [
    "node_modules/*",
    "**/*-aot.ts"
  ]
}